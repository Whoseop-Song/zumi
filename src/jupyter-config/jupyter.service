[Unit]
Description=Jupyter Notebook

[Service]
Type=simple
PIDFile=/run/jupyter.pid

ExecStart=/usr/local/bin/jupyter-notebook --config=/home/pi/zumi/src/jupyter-config/jupyter_notebook_config.py
User=pi
Group=pi
WorkingDirectory=/home/pi/zumi
Restart=always
RestartSec=10
#KillMode=mixed

[Install]
WantedBy=multi-user.target
